{
  "name": "Practice.now()",
  "version": "0.0.1",
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "manifest_version": 2,
  "key": "",
  "description": "Microlearning for Programming Languages",
  "icons": {
    "512": "logo512.png"
  },
  "browser_action": {
    "default_icon": "logo512.png",
    "default_popup": "index.html",
    "default_title": "Practice.now() â€“ quick programming practice"
  },
  "permissions": [
