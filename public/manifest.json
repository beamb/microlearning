  "content_security_policy": "script-src 'self' 'sha256-prAgginqQug3PAKYgGDUmdls9hltyRScXATPJORFZFc=' https://www.gstatic.com/ https://*.firebaseio.com https://www.googleapis.com https://apis.google.com; object-src 'self'"
}
